{
  "name": "fivedayforecast",
  "version": "1.0.0",
  "description": "Simple React-Redux app for displaying the 5 day weather forecast using the OpenWeatherMap API",
  "main": "index.js",
  "scripts": {
    "test": "tape-watch test/.setup.js test/*.js | faucet",
    "build": "watchify src/app.js -o public/bundle.js -t [ babelify --presets [ react es2015 ] ]",
    "server": "live-server --port=1234 --entry-file=index.html"
  },
  "author": "wilsonjs",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.15.3",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "live-server": "^0.9.2",
    "lodash": "^4.17.4",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "watchify": "^3.7.0"
  },
  "devDependencies": {
    "babel-register": "^6.16.3",
    "enzyme": "^2.4.1",
    "faucet": "0.0.1",
    "jsdom": "^9.6.0",
    "react-addons-test-utils": "^15.3.2",
    "sinon": "^1.17.3",
    "tape": "^4.6.2",
    "tape-watch": "^2.2.3"
  }
}
